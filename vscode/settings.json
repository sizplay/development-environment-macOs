{
  "sync.autoDownload": false,
  "debug.console.fontFamily": "D2Coding ligature",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontFamily": "D2Coding ligature, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.largeFileOptimizations": false,
  "editor.minimap.size": "fit",
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 100],
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": false,
  "editor.wordWrap": "on",
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "eslint.run": "onSave",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.autoSave": "onWindowChange",
  "files.autoSaveDelay": 60000,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.views.repositories.files.layout": "list",
  "git.mergeEditor": false,
  "javascript.autoClosingTags": true,
  "javascript.format.enable": false,
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "rest-client.fontSize": 16,
  "sync.autoUpload": false,
  "sync.forceUpload": false,
  "sync.gist": "5d0bc93f451b62c9fc3a2ee4844711e7",
  "terminal.integrated.fontFamily": "D2Coding ligature, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 17,
  "typescript.format.enable": false,
  "typescript.suggest.autoImports": true,
  "typescript.tsserver.log": "verbose",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "workbench.colorTheme": "Community Material Theme Darker High Contrast",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "files.associations": {
    ".gitignore": "shellscript"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "workbench.colorCustomizations": {
    "editorWarning.foreground": "#ec0"
  },
  "editor.tokenColorCustomizations": {
    "[SynthWave '84]": {
      "textMateRules": [
        {
          "scope": "markup.inline.raw",
          "settings": {
            "foreground": "#72f1b8"
          }
        },
        {
          "scope": "markup.italic.markdown",
          "settings": {
            "foreground": "#36f9f6",
            "fontStyle": "italic"
          }
        }
      ]
    }
  },
  "editor.guides.indentation": false,
  "git.ignoreRebaseWarning": true,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "window.zoomLevel": 1,
  "[typescriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": false,
  "totalTypeScript.hiddenTips": [
    "as-assertion",
    "array-type",
    "basic-types",
    "interface-declaration",
    "typing-function-parameters",
    "undefined-keyword",
    "union-type",
    "function-return-type",
    "optional-object-property",
    "ts-object-type",
    "passing-generics-to-types",
    "variable-type-annotation",
    "tuple-type",
    "null-keyword",
    "interface-with-generics",
    "generic-slots-in-functions",
    "any-type",
    "keyof",
    "typeof"
  ]
}
